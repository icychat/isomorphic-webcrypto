{
  "name": "isomorphic-webcrypto",
  "version": "1.1.0",
  "description": "isomorphic webcrypto for IE11+ in 3kB",
  "main": "index.js",
  "module": "index.mjs",
  "browser": {
    "./index.js": "browser.js",
    "./index.mjs": "browser.mjs"
  },
  "types": "index.d.ts",
  "scripts": {
    "build": "cp ./node_modules/webcrypto-shim/webcrypto-shim.js webcrypto-shim.js && echo \"export default {}\" >> webcrypto-shim.js",
    "prepublish": "npm run build",
    "release": "npm run build && git commit -am $npm_package_version && git tag $npm_package_version && git push && git push --tags && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kevlened/isomorphic-webcrypto.git"
  },
  "keywords": [
    "isomorphic",
    "webcrypto",
    "small"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kevlened/isomorphic-webcrypto/issues"
  },
  "homepage": "https://github.com/kevlened/isomorphic-webcrypto#readme",
  "dependencies": {
    "@trust/webcrypto": "^0.8.2",
    "webcrypto-shim": "^0.1.2"
  }
}
